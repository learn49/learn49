{
  "name": "learn49-api",
  "version": "1.0.1",
  "description": "",
  "author": "ID49",
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "postinstall": "nest build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=test jest -i --forceExit --detectOpenHandles --verbose --config ./__tests__/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:test": "ts-node __tests__/seed/admin.ts && NODE_ENV=test jest -i  --forceExit --detectOpenHandles --verbose --config ./__tests__/jest.config.js",
    "migration:new": "typeorm migration:create -n",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:run": "npm run typeorm:cli -- migration:run"
  },
  "dependencies": {
    "@anchan828/nest-sendgrid": "^0.1.39",
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/graphql": "^10.0.2",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/typeorm": "^8.0.3",
    "@sendgrid/mail": "^6.5.1",
    "@sentry/node": "^5.26.0",
    "@sentry/tracing": "^5.26.0",
    "@types/qs": "^6.9.1",
    "apollo-server": "^3.6.3",
    "apollo-server-express": "^3.6.2",
    "aws-sdk": "^2.610.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "graphql": "^16.3.0",
    "graphql-query-complexity": "^0.11.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^4.0.6",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^8.0.7",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "pg": "^8.0.3",
    "qs": "^6.9.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^7.2.0",
    "sharp": "^0.30.1",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.43",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.6",
    "@nestjs/testing": "^8.3.0",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/sharp": "^0.25.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^24.9.0",
    "jest-serial-runner": "^1.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
